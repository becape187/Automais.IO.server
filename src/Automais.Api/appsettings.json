{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD};Ssl Mode=Require"
  },
  "Database": {
    "RootCertificatePath": "ca-certificate.crt"
  },
  "ChirpStack": {
    "ApiUrl": "${CHIRPSTACK_API_URL}",
    "ApiToken": "${CHIRPSTACK_API_TOKEN}"
  },
  "EMQX": {
    "BrokerUrl": "${EMQX_BROKER_URL}",
    "BrokerPort": 1883,
    "WebSocketPort": 8083,
    "Username": "${EMQX_USERNAME}",
    "Password": "${EMQX_PASSWORD}"
  },
  "Backup": {
    "StoragePath": "/backups/routers"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*"
}
